org.apache.hive:hive-jdbc:4.0.0-alpha-1
df_channel = spark.read.format('jdbc')\
    .option('url','jdbc:hive2://192.168.1.39:10000/testdb')\
    .option('dbtable', 'employee' )\
    .option("user", "root") \
    .option("password", "fighting") \
    .load()
df_channel.show()
